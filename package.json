{
    "name": "electron-ts",
    "version": "1.0.0",
    "description": "Electron boilerplate with modular IPC mechanism",
    "main": "dist/main/main.js",
    "scripts": {
        "dev:renderer": "cd renderer && next dev",
        "dev:electron": "NODE_ENV=development electron .",
        "dev": "npm run build:main && npm run build:preload && npm run dev:renderer & npm run dev:electron",
        "build:main": "tsc -p tsconfig.main.json",
        "build:preload": "tsc -p tsconfig.preload.json",
        "build:renderer": "cd renderer && next build",
        "build": "npm run build:main && npm run build:preload && npm run build:renderer",
        "format": "prettier --write \"src/**/*.ts\" \"renderer/**/*.{ts,tsx,css}\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"renderer/**/*.{ts,tsx,css}\"",
        "package": "npm run build && electron-builder"
    },
    "keywords": [
        "electron",
        "typescript",
        "nextjs",
        "react"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-transcribe-streaming": "^3.936.0",
        "@google/genai": "^1.30.0",
        "@google/generative-ai": "^0.24.1",
        "@types/express": "^5.0.5",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "electron": "^39.2.3",
        "express": "^5.1.0",
        "mongoose": "^9.0.0",
        "next": "^16.0.3",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "socket.io": "^4.8.1",
        "tsx": "^4.20.6"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "electron-builder": "^26.0.12",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "type": "module"
}
